algoritmo "semnome"
// Função :
// Autor :
// Data : 15/10/2019
// Seção de Declarações 
var

resposta, n1, n2: inteiro

inicio
// Seção de Comandos

escreva("Digite um numero: ")
leia(n1)

escreva("Digite um numero: ")
leia(n2)

resposta:= n1 div n2

escreva("A parte inteira eh:", resposta)


fimalgoritmo
